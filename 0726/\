def solution(S: str):
    answer= 0.00
    for e in S:
        if e == '*':
            answer*= e
        elif e == '/':
            answer/= e
        elif e == '+':
            answer+= e
        elif e == '-':
            answer-= e
        else:
            e= int(e)

print(solution("2*3+5/6*3+15"))
